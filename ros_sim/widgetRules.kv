#:kivy 1.0.9

<SimCar>:
    size = 50, 50
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size

<Simulator>:
    car: sim_car

    canvas:
        Rectangle:
            pos: self.center_x-5, 0
            size: 10,self.height

    SimCar:
        id: sim_car
        center: self.parent.center